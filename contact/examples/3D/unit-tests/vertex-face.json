    "body_params": [{
        "id": 1,
        "E": 1e5,
        "nu": 0.4,
        "rho": 1000
    }, {
        "id": 2,
        "E": 1e5,
        "nu": 0.4,
        "rho": 1000
    }],

    "export": {
