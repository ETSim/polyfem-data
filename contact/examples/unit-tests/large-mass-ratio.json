        "position": [0, 0.45, 0],
        "scale": 1,
        "body_id": 1
    }, {
        "name": "small-cube",
        "mesh": "../../meshes/simple/cube.msh",
        "position": [0, -0.15, 0],
        "scale": 0.1,
        "body_id": 2
    }, {
        "name": "ground",
        "#mesh": "../../meshes/simple/plane.obj",
        "mesh": "../../meshes/simple/cube.msh",
        "position": [0, -0.25, 0],
        "scale": [2, 0.01, 2],
        "body_id": 3,
        "boundary_id": 1
    }],
    "normalize_mesh": false,

    "body_params": [{
        "name": "large-cube",
        "id": 1,
        "E": 1e8,
        "nu": 0.4,
        "rho": 1e4
    }, {
        "name": "small-cube",
        "id": 2,
        "E": 1e6,
        "nu": 0.4,
        "rho": 1e3
    }, {
        "name": "ground",
        "id": 3,
        "E": 2e11,
        "nu": 0.3,
        "rho": 7750
    }],

    "boundary_sidesets": [{
        "id": 1,
        "axis": -2,
        "position": -0.24
    }],

    "export": {
        "paraview": "sol.vtu",
        "body_ids": true,
        "material_params": true
    },
    "save_time_sequence": true,
    "save_solve_sequence_debug": false,
    "vismesh_rel_area": 10000000
